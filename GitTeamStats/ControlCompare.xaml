            <ListBoxItem Content="Murphy"/>
            <ListBoxItem Content="Kirk"/>
            <ListBoxItem Content="Piggy"/>
            <ListBoxItem Content="Gandalf"/>
        </ListBox>
        <Label Content="Selected Contributors" HorizontalAlignment="Left" Margin="482,63,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label x:Name="lblEmail" Content="Rex (14203)" HorizontalAlignment="Left" Margin="249,100,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblNumOfCommits" Content="Piggy (1203)" HorizontalAlignment="Left" Margin="249,142,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblPercentOfCommits" Content="Kirk (214)" HorizontalAlignment="Left" Margin="249,184,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblLineAdditions" Content="Gandalf (1)" HorizontalAlignment="Left" Margin="249,226,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblLineDeletions" Content="Piggy (24)" HorizontalAlignment="Left" Margin="249,268,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblNumOfFilesEdited" Content="Rex (0)" HorizontalAlignment="Left" Margin="249,310,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblName_Copy" Content="Most Commits:" HorizontalAlignment="Left" Margin="10,58,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblEmail_Copy" Content="Most Line Additions:" HorizontalAlignment="Left" Margin="10,100,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblNumOfCommits_Copy" Content="Most Line Deletions:" HorizontalAlignment="Left" Margin="10,142,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblPercentOfCommits_Copy" Content="Most Files Edited:" HorizontalAlignment="Left" Margin="10,184,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblLineAdditions_Copy" Content="Least Commits:" HorizontalAlignment="Left" Margin="10,226,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblLineDeletions_Copy" Content="Least Line Additions:" HorizontalAlignment="Left" Margin="10,268,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblNumOfFilesEdited_Copy" Content="Least Line Deletions" HorizontalAlignment="Left" Margin="10,310,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblNumOfFilesEdited_Copy1" Content="Gandalf (1)" HorizontalAlignment="Left" Margin="249,352,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
        <Label x:Name="lblNumOfFilesEdited_Copy2" Content="Least Line Deletions" HorizontalAlignment="Left" Margin="10,352,0,0" VerticalAlignment="Top" FontSize="20" Width="239"/>
